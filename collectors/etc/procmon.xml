<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="configuration.xsl"?>

<configuration>
  <processes>
<!-- List all the processes to monitor -->
<process>
        <cmdline>.*org.apache.hadoop.mapred.YarnChild.*</cmdline>
        <displayname>YarnChild</displayname>
    </process>

  </processes>
  <hosts>
    <hostname>localhost</hostname>
  </hosts>
</configuration>
